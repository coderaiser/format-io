{
  "name": "format-io",
  "version": "1.0.3",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "Format size, permissions",
  "homepage": "http://github.com/coderaiser/format-io",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/format-io.git"
  },
  "keywords": [
    "format",
    "size",
    "permissions"
  ],
  "scripts": {
    "wisdom": "npm run build",
    "lint": "redrun lint:*",
    "lint:lib": "eslint lib",
    "lint:test": "eslint -c .eslintrc.test --no-eslintrc test",
    "test": "tape 'test/**/*.js'",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test",
    "watch:lint": "npm run watcher -- 'npm run lint'"
  },
  "dependencies": {
    "currify": "^4.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "lib/format.js",
  "devDependencies": {
    "coveralls": "^3.0.1",
    "eslint": "^5.6.1",
    "eslint-plugin-node": "^10.0.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.17.5",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2"
  }
}
